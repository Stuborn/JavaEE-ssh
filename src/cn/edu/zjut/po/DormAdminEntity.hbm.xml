<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.edu.zjut.po.DormAdminEntity" table="dorm_admin" schema="javaee">
        <id name="daId" column="da_id"/>
        <many-to-one name="accountByUserId" class="cn.edu.zjut.po.AccountEntity">
            <column name="user_id"/>
        </many-to-one>
        <many-to-one name="dormByDormId" class="cn.edu.zjut.po.DormEntity">
            <column name="dorm_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>